{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php-laravel": "~3.0",
        "baopham/dynamodb": "^4.1",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "dumpk/elastcoder": "^0.9.0",
        "facebook/graph-sdk": "^5.6",
        "fideloper/proxy": "~3.3",
        "google/apiclient": "^2.2",
        "google/cloud-storage": "^1.0",
        "intervention/image": "^2.4",
        "janyksteenbeek/youtubepartner": "^1.1",
        "kouz/laravel-airbrake": "^0.3.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "league/csv": "^9.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "michaeljwright/aws-rekognition": "^0.2",
        "nao-pon/flysystem-google-drive": "~1.1",
        "pbmedia/laravel-ffmpeg": "^1.3.0",
        "ralouphie/mimey": "^1.0",
        "schoooch/youtube": "^5.0",
        "talvbansal/media-manager": "^2.5",
        "thujohn/twitter": "^2.2",
        "predis/predis": "^1.1",
        "doctrine/dbal": "^2.6"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "google/cloud-tools": "^0.6",
        "laravel/dusk": "^2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/browser-kit": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Libraries/Hvconfig.php",
            "app/Libraries/ImageHandler.php",
            "app/Libraries/NestedHelper.php",
            "app/Libraries/TextHelper.php",
            "app/Libraries/ThemeHelper.php",
            "app/Libraries/TimeHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "chmod -R 755 bootstrap\/cache",
            "php artisan cache:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
