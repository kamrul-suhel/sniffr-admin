{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "aws/aws-sdk-php-laravel": "~3.0",
        "baopham/dynamodb": "^4.1",
        "dawson/youtube": "^4.1",
        "dumpk/elastcoder": "^0.9.0",
        "facebook/graph-sdk": "^5.6",
        "fideloper/proxy": "~3.3",
        "google/cloud-storage": "^1.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "league/csv": "^9.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "pbmedia/laravel-ffmpeg": "^1.3.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/browser-kit": "^3.0",
        "google/cloud-tools":"^0.6"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Libraries/Hvconfig.php",
            "app/Libraries/ImageHandler.php",
            "app/Libraries/NestedHelper.php",
            "app/Libraries/TextHelper.php",
            "app/Libraries/ThemeHelper.php",
            "app/Libraries/TimeHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "chmod -R 755 bootstrap\/cache",
            "php artisan cache:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
